import java.util.*;
public class Dvgs {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int ty=sc.nextInt();
        for(int k=0;k<ty;k++){
            char cy=sc.next().charAt(0);
            if(cy=='c' || cy=='o' || cy=='d' || cy=='e' || cy=='f' || cy=='o' || cy=='r' || cy=='e' || cy=='s')
                System.out.println("YES");
            else
                System.out.println("NO");
        }
        sc.close();
    }
}