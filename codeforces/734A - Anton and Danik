import java.util.*;
public class Play
{
      public static void main(String args[])
      {
            Scanner sc=new Scanner(System.in);
            int nfbvdbj=sc.nextInt();
            String sbfhb=sc.next();
            int abvvdhb=0,dnbvhbfh=0;
            for(int i=0;i<sbfhb.length();i++)
            {
                  char cbfh=sbfhb.charAt(i);
                  if(cbfh=='A')
                        abvvdhb++;
                  if(cbfh=='D')
                        dnbvhbfh++;
            }
            if(abvvdhb>dnbvhbfh)
                  System.out.println("Anton");
            else if(abvvdhb<dnbvhbfh)
                  System.out.println("Danik");
            else
                  System.out.println("Friendship");
            sc.close();
      }
}